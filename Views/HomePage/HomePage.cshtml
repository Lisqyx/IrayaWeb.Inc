@{
    ViewBag.Title = "HomePage";
}
<style>
    import url('https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;600&display=swap');
    body, html {
        font-family: 'Poppins', sans-serif;
        height: 100%;
        margin: 0;
        overflow-x: hidden;
    }
    :root {
        --primary-dark: #1a3a5f; /* Dark blue */
        --primary-medium: #2c5f8e; /* Medium blue */
        --primary-light: #4b7dbb; /* Light blue */
        --accent: #e9b44c; /* Gold accent */
        --accent-hover: #d4973b; /* Darker gold for hover states */
        --text-light: #ffffff; /* White text */
        --text-dark: #333333; /* Dark gray text */
        --background-light: #f5f7fa; /* Light background */
        --overlay-dark: rgba(26, 58, 95, 0.8); /* Dark overlay */
        --primary-dark: #1a3a5f;
        --text-light: #ffffff;
        --accent: #ffc107;
        --accent-hover: #e0a800;
    }
    .parallax-hero {
        height: 90vh;
        position: relative;
        background-attachment: fixed;
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
        background-image: url('@Url.Content("~/Content/Images/cover.jpg")');
        box-shadow: inset 0 0 0 2000px rgba(26, 58, 95, 0.4);
    }
    .hero-content {
        position: absolute;
        bottom: 20%;
        left: 10%;
        color: var(--text-light);
        text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.7);
        margin-bottom:150px;
    }
    .hero-content h1 {
        font-size: 4rem;
        margin-bottom: 1rem;
        color: var(--text-light);
    }
    .hero-content p {
        font-size: 1.5rem;
        max-width: 600px;
        color: var(--text-light);
    }
    .parallax-section {
        position: relative;
        background-attachment: fixed;
        background-position: center;
        background-repeat: no-repeat;
        background-size: cover;
        height:auto;
    }
    .content-section {
        position: relative;
        padding: 5rem 0;
    }
    .section-content {
        background-color: rgba(255, 255, 255, 0.8);
        padding: 2rem;
        border-radius: 8px;
    }
    /*NAVBAR**/
    .navbar {
        background-color: var(--primary-dark);
        padding: 15px 40px;
        font-family:'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
        word-spacing: 2px;
        font-size: 24px;
        position: fixed;
        width: 100%;
        z-index: 1000;
    }
    .navbar-brand {
        color: var(--text-light);
        margin-left: 10px;
    }
    .navbar-nav .nav-item {
        margin: 0 15px;
    }
    .navbar-nav .nav-item .nav-link {
        color: var(--text-light);
        padding: 8px 12px;
    }
    /*ABOUT SECTION*/
    .about-section {
        background-color: var(--background-light);
        padding: 4rem 0;
        z-index: 1;
        font-size:30px;
    }

        .about-section h1 {
            color: var(--primary-dark);
        }
/*SERVICES SECTION*/
.services-slide {
    height: 500px;
    position: relative;
    color: var(--text-light);
}

.services-overlay {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(26, 58, 95, 0.8);
    display: flex;
    align-items: center;
}

.service-title {
    font-size: 2.5rem;
    font-weight: 600;
    color: var(--accent);
    text-shadow: 2px 2px 4px rgba(0, 0, 0, 0.7);
    position: relative;
    padding-bottom: 15px;
    margin-bottom: 20px;
}

.service-title:after {
    content: '';
    position: absolute;
    bottom: 0;
    left: 0;
    width: 80px;
    height: 4px;
    background-color: var(--accent);
}

.service-list {
    list-style: none;
    padding: 0;
    margin: 0;
}

.service-list li {
    margin-bottom: 20px;
    position: relative;
    padding-left: 20px;
}

.service-list li::before {
    content: '•';
    color: var(--accent);
    font-size: 24px;
    position: absolute;
    left: 0;
    top: -2px;
}

.service-list h4 {
    font-size: 1.3rem;
    margin-bottom: 5px;
    color: var(--accent);
}

.service-list p {
    font-size: 1rem;
    color: rgba(255, 255, 255, 0.9);
}

.service-btn {
    display: inline-block;
    background-color: var(--accent);
    color: var(--primary-dark);
    padding: 10px 25px;
    border-radius: 5px;
    font-weight: 600;
    text-decoration: none;
    transition: all 0.3s ease;
}

.service-btn:hover {
    background-color: var(--accent-hover);
    transform: translateY(-3px);
    box-shadow: 0 5px 15px rgba(0, 0, 0, 0.2);
    color: var(--primary-dark);
}
#servicesCarousel .carousel-indicators {
    bottom: 20px;
}
#servicesCarousel .carousel-indicators button {
    width: 12px;
    height: 12px;
    border-radius: 50%;
    margin: 0 5px;
    background-color: rgba(255, 255, 255, 0.5);
}

#servicesCarousel .carousel-indicators button.active {
    background-color: var(--accent);
}

media (max-width: 992px) {
    .service-title {
        font-size: 2rem;
        text-align: center;
    }

    .service-title:after {
        left: 50%;
        transform: translateX(-50%);
    }
}

media (max-width: 768px) {
    .services-slide {
        height: auto;
        min-height: 600px;
    }

    .services-overlay {
        padding: 60px 0;
    }

    .service-title {
        margin-bottom: 30px;
    }

    .service-list li {
        margin-bottom: 15px;
    }

    .service-list h4 {
        font-size: 1.2rem;
    }
}
    /*PROJECT SECTION*/
    .projects-section {
        text-align: center;
        padding: 50px;
        background-color: var(--background-light);
        z-index: 1;
    }

        .projects-section h2 {
            color: var(--primary-dark);
        }
    .projects-container {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        align-items: stretch;
        gap: 20px;
    }
    .project-card {
        flex: 1;
        display: flex;
        flex-direction: column;
        justify-content: space-between;
        text-align: center;
        background: white;
        border-radius: 8px;
        box-shadow: 2px 2px 10px rgba(0, 0, 0, 0.1);
        overflow: hidden;
        border: 1px solid #e0e0e0;
    }
    .project-card img {
        width: 100%;
        height: 300px;
        object-fit: cover;
    }
    .project-title {
        flex-grow: 1;
        display: flex;
        align-items: center;
        justify-content: center;
        padding: 15px;
        font-size: 18px;
        font-weight: bold;
        color: var(--primary-dark);
        text-align: center;
    }
    .view-all-btn {
        margin-top: 30px;
        padding: 12px 20px;
        background: var(--primary-medium);
        color: var(--text-light);
        border: none;
        cursor: pointer;
        border-radius: 5px;
        font-size: 16px;
        transition: 0.3s;
    }
    .view-all-btn:hover {
        background: var(--primary-dark);
    }
    .scroll-down {
        position: absolute;
        bottom: 30px;
        left: 50%;
        transform: translateX(-50%);
        animation: bounce 2s infinite;
        color: var(--text-light);
        cursor: pointer;
    }
    keyframes bounce {
        0%, 20%, 50%, 80%, 100% {transform: translateY(0) translateX(-50%);}
        40% {transform: translateY(-30px) translateX(-50%);}
        60% {transform: translateY(-15px) translateX(-50%);}
        }
    /* LOCATION SECTION */
.locations-section {
    padding: 80px 0;
    position: relative;
    background-attachment: fixed;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
    background-image: url('@Url.Content("~/Content/Images/DarkenService.jpg")');
}

.locations-section::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(26, 58, 95, 0.75);
    z-index: 0;
}

.locations-section .container {
    position: relative;
    z-index: 1;
}

.locations-section .section-title {
    color: var(--text-light);
    font-size: 2.5rem;
    font-weight: 500;
    margin-bottom: 20px;
    text-align: center;
}

.locations-section .section-description {
    color: var(--text-light);
    max-width: 800px;
    margin: 0 auto 40px;
    font-size: 1.2rem;
    text-align: center;
}

.locations-section .carousel-item .card {
    border: none;
    border-radius: 15px;
    overflow: hidden;
}

.locations-section .carousel-item .card-img {
    height: 400px;
    object-fit: cover;
    opacity: 0.6;
}

.locations-section .card-img-overlay {
    display: flex;
    flex-direction: column;
    justify-content: center;
    text-align: center;
}

.locations-section .card-title {
    font-size: 2.5rem;
    font-weight: 600;
    text-shadow: 2px 2px 4px rgba(0,0,0,0.7);
}

.locations-section .location-info {
    background-color: rgba(26, 58, 95, 0.8);
    border-radius: 10px;
    padding: 20px;
    max-width: 500px;
    margin: 0 auto;
}

.locations-section .card-text {
    font-size: 1.2rem;
    margin-bottom: 10px;
}

.locations-section .card-text i {
    color: var(--accent);
    margin-right: 10px;
}

.locations-section .card-text:last-child {
    margin-bottom: 0;
}

.locations-section .map-button {
    text-align: center;
    margin-top: 30px;
}

.locations-section .map-btn {
    background-color: var(--accent);
    color: var(--primary-dark);
    padding: 12px 25px;
    border-radius: 5px;
    font-weight: 500;
    transition: all 0.3s ease;
    display: inline-block;
    text-decoration: none;
}
media (max-width: 768px) {
    .locations-section .card-title {
        font-size: 2rem;
    }

    .locations-section .card-text {
        font-size: 1rem;
    }

    .locations-section .section-title {
        font-size: 2rem;
    }

    .locations-section .section-description {
        font-size: 1rem;
    }
}
    /*CLIENT SECTION*/
        .clients-section {
        background-color: var(--background-light);
        padding: 80px 0;
        text-align: center;
    }
    .section-title {
        font-size: 2.5rem;
        margin-bottom: 20px;
        font-weight: 500;
        color: var(--primary-dark);
    }
    .clients-description {
        max-width: 800px;
        margin: 0 auto 50px;
    }
    .clients-description p {
        font-size: 1.2rem;
        color: var(--text-dark);
        line-height: 1.6;
    }
    .clients-grid {
        display: flex;
        flex-direction: column;
        gap: 40px;
        max-width: 1100px;
        margin: 0 auto 60px;
    }
    .client-category {
        background-color: white;
        border-radius: 10px;
        padding: 30px;
        box-shadow: 0 5px 15px rgba(0, 0, 0, 0.05);
        transition: transform 0.3s;
    }
    .category-title {
        font-size: 1.5rem;
        margin-bottom: 25px;
        color: var(--primary-medium);
        position: relative;
        display: inline-block;
    }
    .category-title:after {
        content: '';
        position: absolute;
        bottom: -8px;
        left: 50%;
        transform: translateX(-50%);
        width: 50px;
        height: 3px;
        background-color: var(--accent);
    }
    .category-clients {
        display: flex;
        flex-wrap: wrap;
        justify-content: center;
        gap: 30px;
    }
    .client-item {
        display: flex;
        align-items: center;
        gap: 20px;
        width: 100%;
        max-width: 450px;
        text-align: left;
        padding: 15px;
        border-radius: 8px;
        transition: background-color 0.3s;
    }
    .client-logo {
        width: 80px;
        height: 80px;
        flex-shrink: 0;
        display: flex;
        align-items: center;
        justify-content: center;
    }
    .client-logo img {
        max-width: 100%;
        max-height: 100%;
        object-fit: contain;
    }
    .client-details {
        flex-grow: 1;
    }
    .client-details h4 {
        font-size: 1.1rem;
        margin-bottom: 8px;
        color: var(--primary-dark);
    }
    .client-details p {
        font-size: 0.9rem;
        color: var(--text-dark);
        line-height: 1.4;
    }
 /*CONTACT SECTION*/
.contact-section {
    padding: 80px 0;
    position: relative;
    background-attachment: fixed;
    background-position: center;
    background-repeat: no-repeat;
    background-size: cover;
    background-image: url('@Url.Content("~/Content/Images/DarkenService.jpg")');
}

.contact-section::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: rgba(26, 58, 95, 0.75);
}

.section-title {
    text-align: center;
    font-size: 2.5rem;
    margin-bottom: 50px;
    font-weight: 500;
    color: #fff;
    position: relative;
    z-index: 2;
}

.contact-container {
    max-width: 1100px;
    margin: 0 auto;
    position: relative;
    z-index: 2;
}

.contact-form-container {
    background-color: rgba(255, 255, 255, 0.95);
    border-radius: 10px;
    padding: 40px;
    box-shadow: 0 5px 20px rgba(0, 0, 0, 0.15);
    display: flex;
    flex-direction: column;
    gap: 30px;
}

.contact-info-grid {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 30px;
    margin-bottom: 10px;
    border-bottom: 1px solid #e0e0e0;
    padding-bottom: 30px;
}
.info-block {
    text-align: center;
}
.contact-icon {
    font-size: 2rem;
    color: var(--primary-medium);
    margin-bottom: 15px;
}

.info-block h3 {
    font-size: 1.3rem;
    color: var(--primary-dark);
    margin-bottom: 10px;
}

.info-block p {
    font-size: 1rem;
    color: var(--text-dark);
    margin-bottom: 5px;
}
    .contact-section .block-link {
        text-decoration: none;
    }
    .contact-section a {
        text-decoration: none;
    }
/* Responsive Design */
media (max-width: 992px) {
    .contact-info-grid {
        grid-template-columns: 1fr;
        gap: 20px;
    }

    .contact-form-container {
        padding: 30px;
    }
}

media (max-width: 768px) {
    .section-title {
        font-size: 2rem;
    }

    .contact-form-container {
        padding: 25px;
    }
}
    /*FOOTER SECTION*/
        .footer-section {
        background-color: var(--primary-dark);
        color: var(--text-light);
        font-family: 'Poppins', sans-serif;
    }
    .footer-main {
        padding: 60px 0 40px;
    }
    .footer-content {
        display: flex;
        flex-wrap: wrap;
        justify-content: space-between;
        gap: 40px;
    }
    .footer-logo-info {
        flex: 1;
        min-width: 250px;
        max-width: 350px;
    }
    .footer-logo {
        color: var(--text-light);
        font-size: 1.8rem;
        margin-bottom: 15px;
    }
    .footer-desc {
        margin-bottom: 20px;
        line-height: 1.6;
        color: rgba(255, 255, 255, 0.8);
    }
  .footer-social {
    justify-content: center;
  }
}
    .social-icon {
        width: 40px;
        height: 40px;
        border-radius: 50%;
        background-color: rgba(255, 255, 255, 0.1);
        display: flex;
        align-items: center;
        justify-content: center;
        color: var(--text-light);
    }
    .footer-links, .footer-services {
        flex: 1;
        min-width: 150px;
    }
    .footer-newsletter {
        flex: 1;
        min-width: 250px;
        max-width: 350px;
    }
    .footer-title {
        font-size: 1.3rem;
        font-weight: 600;
        margin-bottom: 20px;
        position: relative;
        padding-bottom: 10px;
    }
    .footer-title:after {
        content: '';
        position: absolute;
        bottom: 0;
        left: 0;
        width: 50px;
        height: 3px;
        background-color: var(--accent);
    }
    .footer-list {
        list-style: none;
        padding: 0;
        margin: 0;
    }
    .footer-list li {
        margin-bottom: 12px;
    }
    .footer-list a {
        color: rgba(255, 255, 255, 0.8);
        text-decoration: none;
        display: inline-block;
    }
    .newsletter-form {
        display: flex;
        margin-bottom: 20px;
    }
    .newsletter-form input {
        flex-grow: 1;
        padding: 12px 15px;
        border: none;
        border-radius: 5px 0 0 5px;
        font-family: 'Poppins', sans-serif;
    }
    .newsletter-btn {
        background-color: var(--accent);
        color: var(--primary-dark);
        border: none;
        border-radius: 0 5px 5px 0;
        padding: 0 15px;
        cursor: pointer;
        transition: background-color 0.3s ease;
    }
    .newsletter-btn:hover {
        background-color: var(--accent-hover);
    }
    .certification-logos img {
        height: 40px;
        width: auto;
        opacity: 0.8;
    }
    .footer-bottom {
        background-color: rgba(0, 0, 0, 0.2);
        padding: 20px 0;
    }
    .footer-bottom .container {
        display: flex;
        justify-content: space-between;
        align-items: center;
    }
    .copyright a {
        color: rgba(255, 255, 255, 0.8);
        text-decoration: none;
        transition: color 0.3s ease;
    }
</style>
