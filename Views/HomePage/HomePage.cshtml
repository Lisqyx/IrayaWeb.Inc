            <div class="carousel-item active">
                <div class="parallax-section services-slide" style="background-image: url('@Url.Content("~/Content/Images/BoundaryLot.png")');">
                    <div class="services-overlay">
                        <div class="container">
                            <div class="row align-items-center">
                                <div class="col-md-4">
                                    <h3 class="service-title">Boundary & Lot Surveys</h3>
                                </div>
                                <div class="col-md-8">
                                    <ul class="service-list">
                                        <li>
                                            <h4>Boundary Verification Survey</h4>
                                            <p>Accurately determine and mark property boundaries based on historical records and physical features</p>
                                        </li>
                                        <li>
                                            <h4>Relocation & Subdivision Survey</h4>
                                            <p>Create legally compliant subdivisions and relocate property boundaries with precision</p>
                                        </li>
                                        <li>
                                            <h4>Land Title & Legal Documentation</h4>
                                            <p>Prepare all necessary documentation for legal property transactions and registry</p>
                                        </li>
                                    </ul>
                                    <div class="text-center mt-4">
                                        <a href="#" class="service-btn">Learn more</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Engineering & Construction Surveys -->
            <div class="carousel-item">
                <div class="parallax-section services-slide" style="background-image: url('@Url.Content("~/Content/Images/EngineerConstruct.jpg")');">
